<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.toy9986619.finalexam.MAP_Activity">

    <EditText
        android:id="@+id/map_sbp"
        android:layout_width="96dp"
        android:layout_height="47dp"
        android:layout_marginLeft="203dp"
        android:layout_marginTop="80dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/map_dbp"
        android:layout_width="95dp"
        android:layout_height="43dp"
        android:layout_marginLeft="203dp"
        android:layout_marginTop="128dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="261dp"
        android:layout_height="49dp"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="7dp"
        android:gravity="center_vertical"
        android:text="Find Your MAP"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/map_calculate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="127dp"
        android:layout_marginTop="171dp"
        android:onClick="Result"
        android:text="Calculate MAP"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="114dp"
        android:layout_height="31dp"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="9dp"
        android:gravity="center_vertical"
        android:text="Your MAP is :"
        android:textAlignment="center"
        android:textSize="17sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/map_calculate" />

    <TextView
        android:id="@+id/map_rangetext"
        android:layout_width="197dp"
        android:layout_height="34dp"
        android:layout_marginBottom="204dp"
        android:layout_marginLeft="94dp"
        android:gravity="center_vertical"
        android:text="Normal Range of MAP"
        android:textAlignment="center"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/map_result"
        android:layout_width="101dp"
        android:layout_height="29dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="0dp"
        android:gravity="center_vertical"
        android:text=" ??.? "
        android:textAlignment="center"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        tools:layout_editor_absoluteY="230dp" />

    <Button
        android:id="@+id/map_send"
        android:layout_width="103dp"
        android:layout_height="48dp"
        android:layout_marginBottom="93dp"
        android:layout_marginLeft="141dp"
        android:onClick="send"
        android:text="Send Mail"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/map_exit"
        android:layout_width="98dp"
        android:layout_height="48dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="12dp"
        android:onClick="Exit"
        android:text="Return"
        app:layout_constraintLeft_toLeftOf="@+id/map_send"
        app:layout_constraintTop_toBottomOf="@+id/map_send" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="154dp"
        android:layout_height="26dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="91dp"
        android:text="Systolic Blood Pressure:"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:layout_marginRight="16dp"
        android:gravity="center_vertical"
        android:text="mmHg"
        android:textAlignment="center"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/map_result"
        android:layout_marginBottom="-7dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:layout_marginRight="7dp"
        android:layout_marginTop="131dp"
        android:gravity="center_vertical"
        android:text="mmHg"
        android:textAlignment="center"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:layout_marginRight="7dp"
        android:layout_marginTop="92dp"
        android:gravity="center_vertical"
        android:text="mmHg"
        android:textAlignment="center"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="161dp"
        android:layout_height="28dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="14dp"
        android:text="Diastolic Blood Pressure:"
        app:layout_constraintLeft_toLeftOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="86dp"
        android:layout_marginTop="0dp"
        android:text="Input Your"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/map_range1"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="14dp"
        android:text="80 mmHg"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/map_rangetext" />

    <TextView
        android:id="@+id/map_range2"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="14dp"
        android:text="100 mmHg"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/map_rangetext" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="38dp"
        android:layout_height="31dp"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="0dp"
        android:text="~"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintLeft_toRightOf="@+id/map_range1"
        app:layout_constraintTop_toTopOf="@+id/map_range1" />

</android.support.constraint.ConstraintLayout>
